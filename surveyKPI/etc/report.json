{
	"version": "1.0",
	"type": "photo" || "video" || "rich",  // Table and map data will be "rich"
	"width": (N) Width in pixels ref oemcode spec,
	"height": (N) Height in pixels ref oemcode spec,
	"title": (S) Report Title,
	"url": (S) Url for type image or video (can be inserted into <img> tag)
	"html": (S) HTML for type "rich",	
	"author_name": (S) Name of person who created report,
	"provider_name": "Smap",
	"provider_url": (S) The url of the smap server,
	"thumbnail_url": (S) Url of a thumbnail,
	"thumbnail_width": (N)
	"thumbnail_height": (N) Height in pixels ref oemcode spec,
	"smap": {
	    "pub_date": (S) Publication date for example: "2013-04-30T22:19:23+0000",
        "start_date": (S) Date of first survey record,
        "end_date": (S) Date of last survey record,
        "data_type": (S) "table" or "graph" or "map",
        "data_url": (S) Url of data 
        "boundingBox": {
            "left":"23.0",
            "bottom":"10.0",
            "right":"32.4",
            "top":"22.0"
        },
	}
}